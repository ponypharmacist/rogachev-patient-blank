                  .col-2.is-hideable-col-1-2.input-cell
                    input#reference-bmi.form-control(type="text" placeholder="..." readonly)

                .row-like.is-hideable-1-5
                  .col-8.thead-col Индекс талия-окружность бедер (WHR) 
                    input#waistLength.form-control.inline-mini.no-print(type="text" placeholder="Талия (см)" tabindex="11")
