            .col-12.is-hideable-table-1
              .table-like.table-like-1
                .row-like.thead-like
                  .col-8 Антропометрические показатели 
                    a#hideable-table-1.element-remover ✕
                  .col-2.text-center Абсолютное значение
                  .col-2.text-center.is-hideable-col-1-2 Референсные значения 
                    a#hideable-col-1-2.element-remover ✕

                .row-like.is-hideable-1-1
                  .col-8.thead-col Рост стоя (см) 
                    a#hideable-1-1.element-remover ✕
                  .col-2.input-cell
                    input#height.form-control(type="text" placeholder="..." maxlength="6" tabindex="7")
                  .col-2.is-hideable-col-1-2.input-cell
                    input#reference-height.form-control(type="text" placeholder="..." readonly)

                .row-like.is-hideable-1-2
                  .col-8.thead-col Рост сидя (см) 
                    a#hideable-1-2.element-remover ✕
                  .col-2.input-cell
                    input#heightSitting.form-control.update-object(type="text" placeholder="..." maxlength="6" tabindex="8")
                  .col-2.is-hideable-col-1-2.input-cell
                    input.form-control(type="text" placeholder="...")

                .row-like.is-hideable-1-3
                  .col-8.thead-col Вес (кг) 
